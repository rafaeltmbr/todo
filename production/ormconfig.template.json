{
  "type": "postgres",
  "host": "$DB_HOST",
  "port": "$DB_PORT",
  "username": "$DB_USER",
  "password": "$DB_PASS",
  "database": "$DB_DB",
  "synchronize": false,
  "logging": false,
  "entities": ["./dist/modules/**/infra/typeorm/entities/*.js"],
  "migrations": ["./dist/shared/infra/typeorm/migrations/*.js"],
  "cli": {
    "migrationsDir": "./dist/shared/infra/typeorm/migrations"
  }
}
